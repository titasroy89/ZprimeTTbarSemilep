#HTC Submission File for SFrameBatch
# +MyProject        =  "af-cms" 
requirements      =  OpSysAndVer == "Centos7"
universe          = vanilla
# #Running in local mode with 8 cpu slots
# universe          =  local
# request_cpus      =  8 
notification      = Error
notify_user       = condor.desy@gmail.com
initialdir        = workdir_Zprime_Analysis_2017v2_CHS
output            = $(Stream)/WZ_2017v2.o$(ClusterId).$(Process)
error             = $(Stream)/WZ_2017v2.e$(ClusterId).$(Process)
log               = $(Stream)/WZ_2017v2.$(Cluster).log
#Requesting CPU and DISK Memory - default +RequestRuntime of 3h stays unaltered
RequestMemory     = 2G
RequestDisk       = 2G
#You need to set up sframe
getenv            = True
environment       = "LD_LIBRARY_PATH_STORED=/nfs/dust/cms/user/hugobg/UHH2_v2/CMSSW_10_2_17/biglib/slc7_amd64_gcc700:/nfs/dust/cms/user/hugobg/UHH2_v2/CMSSW_10_2_17/lib/slc7_amd64_gcc700:/nfs/dust/cms/user/hugobg/UHH2_v2/CMSSW_10_2_17/external/slc7_amd64_gcc700/lib:/cvmfs/cms.cern.ch/slc7_amd64_gcc700/cms/cmssw/CMSSW_10_2_17/biglib/slc7_amd64_gcc700:/cvmfs/cms.cern.ch/slc7_amd64_gcc700/cms/cmssw/CMSSW_10_2_17/lib/slc7_amd64_gcc700:/cvmfs/cms.cern.ch/slc7_amd64_gcc700/cms/cmssw/CMSSW_10_2_17/external/slc7_amd64_gcc700/lib:/cvmfs/cms.cern.ch/slc7_amd64_gcc700/external/llvm/6.0.0-ogkkac/lib64:/cvmfs/cms.cern.ch/slc7_amd64_gcc700/external/gcc/7.0.0-omkpbe2/lib64:/cvmfs/cms.cern.ch/slc7_amd64_gcc700/external/gcc/7.0.0-omkpbe2/lib:/cvmfs/cms.cern.ch/slc7_amd64_gcc700/external/cuda/9.2.148/drivers:/nfs/dust/cms/user/hugobg/UHH2/SFrame/lib:/cvmfs/grid.cern.ch/centos7-umd4-ui-4_200423/lib64:/cvmfs/grid.cern.ch/centos7-umd4-ui-4_200423/lib:/cvmfs/grid.cern.ch/centos7-umd4-ui-4_200423/usr/lib64:/cvmfs/grid.cern.ch/centos7-umd4-ui-4_200423/usr/lib:/cvmfs/grid.cern.ch/centos7-umd4-ui-4_200423/usr/lib64/dcap"
